@model  IEnumerable<SAC.Models.ChequeModelView>


<div class="table-responsive">
    <table id="TblChequeCliente" class="table stylish-table">
        <thead>
            <tr>
                <th class="text-center"> </th>
                <th class="text-center"> Número</th>
                <th class="text-center"> Banco</th>
                <th class="text-center"> Fecha</th>
                <th class="text-center"> Importe</th>
                @*<th class="text-center"> Moneda</th>*@
            </tr>
        </thead>
        <tbody>



            @if (Model.Count() > 0)
            {
                foreach (var item in Model)
                {
                    <tr id="@item.Id" import="@item.Importe">
                        <td>
                            <a href="#" data-toggle="tooltip" data-original-title="Eliminar "
                               class="btn btn-danger btn-xs fa fa-trash-o "
                               onclick="ConfirmaEliminarCheque(@item.Id)">
                            </a>
                        </td>
                        <td class="text-right">@Html.DisplayFor(modelItem => item.NumeroCheque)</td>
                        <td class="text-center">@Html.DisplayFor(modelItem => item.BancoCheque.Nombre)</td>
                        <td class="text-center">@Html.DisplayFor(modelItem => item.Fecha)</td>
                        <td class="text-right">@Html.DisplayFor(modelItem => item.Importe)</td>
                        @*<td class="te
                                  }
                            xt-center">@Html.DisplayFor(modelItem => item.TipoMoneda.Descripcion)</td>*@
                    </tr>
                }
            }
            else
            {
                <tr><td colspan="5">No se se encuentran datos...</td></tr>
            }



        </tbody>

    </table>
</div>





